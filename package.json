  "scripts": {
    "start-dev": "./node_modules/nodemon/bin/nodemon.js --trace-warnings ./server.js --config nodemon.json",
    "start": "node src/index.js",
    "migrate": "NODE_ENV=development sequelize db:migrate --config src/config/databases.js --migrations-path src/db/migrations",
    "docker:migrate": "NODE_ENV=development docker exec -it resonate-api npx sequelize db:migrate --config src/config/databases.js --migrations-path src/db/migrations",
    "docker:migrate:undo:all": "NODE_ENV=development docker exec -it resonate-api npx sequelize db:migrate:undo:all --config src/config/databases.js --migrations-path src/db/migrations",
    "docker:seed:all": "NODE_ENV=development docker exec -it resonate-api npx sequelize db:seed:all --config src/config/databases.js --seeders-path src/db/seeders",
    "docker:seed:undo:all": "NODE_ENV=development docker exec -it resonate-api npx sequelize db:seed:undo:all --config src/config/databases.js --seeders-path src/db/seeders",
    "test": "npm run test:lint src",
    "test:lint": "standard src"
  },
