{
  "name": "etanoser",
  "version": "0.0.1",
  "description": "Etanoser",
  "main": "index.mjs",
  "author": "Si <simsamsomred@gmail.com>",
  "license": "gpl3",
  "private": true,
  "scripts": {
    "start:dev": "./node_modules/nodemon/bin/nodemon.js --trace-warnings ./server.mjs --config nodemon.json",
    "start": "node src/index.mjs",
    "migrate": "NODE_ENV=development sequelize db:migrate --config src/config/databases.js --migrations-path src/db/migrations",
    "docker:workers": "docker exec -it resonate-api node src/jobs/queue-worker.js run",
    "docker:migrate": "docker exec -it resonate-api npx sequelize db:migrate --config src/config/databases.js --migrations-path src/db/migrations",
    "docker:migrate:undo:all": "docker exec -it resonate-api npx sequelize db:migrate:undo:all --config src/config/databases.js --migrations-path src/db/migrations",
    "docker:seed:all": "docker exec -it resonate-api npx sequelize db:seed:all --config src/config/databases.js --seeders-path src/db/seeders",
    "docker:seed:undo:all": "docker exec -it resonate-api npx sequelize db:seed:undo:all --config src/config/databases.js --seeders-path src/db/seeders",
    "docker:compose:test:up": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up",
    "docker:compose:test:down": "docker-compose -f docker-compose.yml -f docker-compose.test.yml down",
    "migrate:test": "NODE_ENV=test sequelize db:migrate --config src/config/databases.js --migrations-path src/db/migrations",
    "docker:workers:test": "docker exec -it resonate-api node src/jobs/queue-worker.js run",
    "docker:seed:all:test": "docker exec -it resonate-api npx sequelize db:seed:all --config src/config/databases.js --seeders-path src/db/seeders/test",
    "docker:seed:undo:all:test": "docker exec -it resonate-api npx sequelize db:seed:undo:all --config src/config/databases.js --seeders-path src/db/seeders/test",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "test:all": "docker exec -it resonate-api yarn mocha test",
    "test:accessTokenExample": "clear && docker exec -it resonate-api yarn mocha test/auth/AccessTokenExample.test.js",
    "test:baseline:all": "clear && docker exec -it resonate-api yarn mocha test/baseline",
    "test:api": "clear && docker exec -it resonate-api yarn mocha test/baseline/Api.ts",
    "test:admin": "clear && docker exec -it resonate-api yarn mocha test/baseline/Admin.ts",
    "test:user": "clear && docker exec -it resonate-api yarn mocha test/baseline/User.ts",
    "postinstall": "husky install"
  },
